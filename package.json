{
  "name": "@whitedog/fdc3-react",
  "version": "0.1.0-alpha.1",
  "author": "White Dog & Company Ltd.",
  "homepage": "https://github.com/whitedogio/fdc3-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/whitedogio/fdc3-react.git"
  },
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/fdc3-react.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "size-limit": [
    {
      "path": "dist/fdc3-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/fdc3-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.6.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.9.8",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "react-dom": "^16.8.0",
    "react-test-renderer": "^16.8.0",
    "size-limit": "^4.6.0",
    "ts-jest": "^26.4.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^3.9.0"
  },
  "dependencies": {
    "@finos/fdc3": "^1.1.0-alpha.3",
    "react": "^16.8.0"
  }
}
